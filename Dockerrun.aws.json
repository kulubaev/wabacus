{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "web",
      "image": "shnoby/calcul-web",
      "hostname": "web",
      "essential": false,
      "memory": 128
    },
    {
      "name": "api",
      "image": "shnoby/calcul-api",
      "hostname": "api",
      "essential": false,
      "links": ["worker"],
      "memory": 128
    },
    {
      "name": "worker",
      "image": "shnoby/calcul-worker",
      "hostname": "worker",
      "essential": false,
      "links": ["api"],
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "shnoby/calcul-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["web", "api"],
      "memory": 128
    }
  ]

}
